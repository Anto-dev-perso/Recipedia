{
  "expo": {
    "name": "RecipesManager",
    "slug": "RecipesManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#006D38",
      "dark": {
        "image": "./src/assets/app/splash.png",
        "backgroundColor": "#79DB95"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recipesmanager",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app/adaptive_icon.png"
      },
      "package": "com.recipesmanager",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      "expo-localization",
      "expo-sqlite",
      [
        "expo-asset",
        {
          "assets": [
            "./src/assets/app",
            "./src/assets/images"
          ]
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "7958883d-ab87-4da5-bccc-fe0c7ff40b6e"
      }
    },
    "owner": "antoc"
  }
}
